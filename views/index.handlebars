    <script>
           $(document).ready(function() {

            //  $$$$$$$$$$$$$$$$$$$$$$$    START MAIN SLIDER       $$$$$$$$$$$$$$$$$$$$$$$-->

            $(".mainSlider #owl-demo").owlCarousel({

                navigation: true, // Show next and prev buttons
                slideSpeed: 300,
                paginationSpeed: 400,
                dots: false,
                nav: true,
                items: 1,
                autoplay: true,
                itemsDesktop: false,
                itemsDesktopSmall: false,
